  "type": "magento2-module",
  "version": "1.0.1",
  "license": [
    "OSL-3.0"
  ],
  "authors": [
    {
      "name": "Magenizr",
      "email": "modules@magenizr.com",
      "homepage": "https://www.magenizr.com"
    }
  ],
  "require": {
    "magento/framework": "^100|^101|^102",
    "magento/module-backend": "^100|^101"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.magento.com/"
    }
  ],
  "keywords": [
    "Magento 2",
    "Admin",
    "Backend",
    "UI Bookmarks",
    "Clear",
    "Flush"
